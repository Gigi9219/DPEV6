<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="App.Votaciones"
             BackgroundImage="MenuUI.png">
    <StackLayout>
        <Grid x:Name="sl" RowSpacing="5" ColumnSpacing="5" >
            <Grid.RowDefinitions>
                <RowDefinition Height="*" />
                <RowDefinition Height="*" />
                <RowDefinition Height="*" />
                <RowDefinition Height="*" />
                <RowDefinition Height="*" />
            </Grid.RowDefinitions>
            <Button x:Name="part1" Grid.Row="2"/>
            <Button x:Name="part2" Grid.Row="3"/>
            <Button x:Name="part3" Grid.Row="4"/>
        </Grid>
    </StackLayout>

</ContentPage>